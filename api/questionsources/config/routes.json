{
  "routes": [
    {
      "method": "GET",
      "path": "/questionsources",
      "handler": "Questionsources.find",
      "config": {
        "policies": [
          "global.isMe"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/questionsources/count",
      "handler": "Questionsources.count",
      "config": {
        "policies": [
          "global.isMe"
        ]
      }
    },
    {
      "method": "GET",
      "path": "/questionsources/:id",
      "handler": "Questionsources.findOne",
      "config": {
        "policies": [
          "checkOwner"
        ]
      }
    },
    {
      "method": "POST",
      "path": "/questionsources",
      "handler": "Questionsources.create",
      "config": {
        "policies": [
          "global.setOwner"
        ]
      }
    },
    {
      "method": "PUT",
      "path": "/questionsources/:id",
      "handler": "Questionsources.update",
      "config": {
        "policies": [
          "checkOwner"
        ]
      }
    },
    {
      "method": "DELETE",
      "path": "/questionsources/:id",
      "handler": "Questionsources.delete",
      "config": {
        "policies": [
          "checkOwner"
        ]
      }
    }
  ]
}
