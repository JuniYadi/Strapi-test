{
  "connection": "default",
  "collectionName": "source",
  "info": {
    "name": "Question Sources",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "sourceInfo": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "year": {
      "type": "string",
      "required": true
    },
    "questions": {
      "via": "questionsource",
      "collection": "questions"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
