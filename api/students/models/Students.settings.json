{
  "connection": "default",
  "collectionName": "students",
  "info": {
    "name": "students",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "studentNumber": {
      "required": true,
      "type": "string",
      "unique": true
    },
    "studentName": {
      "required": true,
      "type": "string"
    },
    "studentBirthday": {
      "type": "date"
    },
    "parentMobilePhone": {
      "type": "string"
    },
    "school": {
      "model": "schools",
      "via": "students"
    },
    "activeStatus": {
      "type": "boolean"
    },
    "class": {
      "model": "classes",
      "via": "students"
    },
    "grade": {
      "model": "grades",
      "via": "students"
    }
  }
}