{
  "connection": "default",
  "collectionName": "chapters",
  "info": {
    "name": "chapters",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "curriculum": {
      "model": "curriculums",
      "via": "chapters"
    },
    "questions": {
      "collection": "questions",
      "via": "chapter"
    },
    "quiz": {
      "model": "quizzes",
      "via": "chapters"
    },
    "SubChapter": {
      "type": "component",
      "repeatable": true,
      "component": "subchapter.sub-chapter"
    }
  }
}
