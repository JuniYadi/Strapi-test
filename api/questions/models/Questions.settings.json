{
  "connection": "default",
  "collectionName": "questions",
  "info": {
    "name": "questions",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "questionType": {
      "required": true,
      "enum": [
        "image",
        "text"
      ],
      "type": "enumeration"
    },
    "questionImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "optionA": {
      "type": "string",
      "required": true
    },
    "optionB": {
      "required": true,
      "type": "string"
    },
    "optionC": {
      "type": "string"
    },
    "optionD": {
      "type": "string"
    },
    "optionE": {
      "type": "string"
    },
    "answer": {
      "enum": [
        "A",
        "B",
        "C",
        "D",
        "E"
      ],
      "type": "enumeration",
      "required": true
    },
    "explanationType": {
      "enum": [
        "image",
        "text",
        "pdf",
        "video"
      ],
      "type": "enumeration",
      "required": true
    },
    "imageExplanation": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "textExplanation": {
      "type": "richtext"
    },
    "pdfExplanation": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "videoExplanation": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "difficulties": {
      "enum": [
        "Easy",
        "Medium",
        "Hard"
      ],
      "type": "enumeration"
    },
    "questionVideo": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "question": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "questionDetail": {
      "type": "richtext",
      "required": true
    },
    "questionsource": {
      "model": "questionsources",
      "via": "questions"
    },
    "chapter": {
      "model": "chapters",
      "via": "questions"
    },
    "Workflow": {
      "component": "workflow.data",
      "type": "component"
    },
    "class": {
      "model": "classes",
      "via": "questions"
    },
    "teacher": {
      "model": "teachers",
      "via": "questions"
    },
    "quizzes": {
      "collection": "quizzes",
      "via": "questions"
    }
  }
}