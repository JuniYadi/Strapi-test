{
  "connection": "default",
  "collectionName": "kompetensidasars",
  "info": {
    "name": "Kompetensi Dasar"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "curriculum": {
      "model": "curriculums"
    },
    "courses": {
      "collection": "courses",
      "via": "kompetensi_dasar"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
