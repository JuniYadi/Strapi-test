{
  "connection": "default",
  "collectionName": "courses",
  "info": {
    "name": "courses",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "icon": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "curriculums": {
      "collection": "curriculums",
      "via": "courses",
      "dominant": true
    },
    "classes": {
      "collection": "classes",
      "via": "courses",
      "dominant": true
    },
    "quizzes": {
      "collection": "quizzes",
      "via": "courses"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
