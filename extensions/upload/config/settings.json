{
  "provider": "aws-s3",
  "providerOptions": {
    "accessKeyId": "${process.env.AWS_S3_KEY_ID || ''}",
    "secretAccessKey": "${process.env.AWS_S3_KEY_SECRET || ''}",
    "region": "${process.env.AWS_S3_REGION || ''}",
    "params": {
      "Bucket": "${process.env.AWS_S3_BUCKET || ''}"
    }
  }
}
